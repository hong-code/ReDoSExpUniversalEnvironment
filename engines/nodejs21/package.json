{
  "name": "regex-benchmark-nodejs21-nonbacktracking",
  "version": "1.1.0",
  "description": "Regex benchmark program using V8's experimental non-backtracking RegExp engine for security and performance",
  "main": "benchmark.js",
  "type": "commonjs",
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "benchmark": "node --enable-experimental-regexp-engine --default-to-experimental-regexp-engine benchmark.js",
    "benchmark-linear": "node --enable-experimental-regexp-engine benchmark.js",
    "benchmark-fallback": "node --default-to-experimental-regexp-engine benchmark.js",
    "test": "cd tests && ./run_tests.sh",
    "simple-test": "cd tests && ./simple_test.sh"
  },
  "keywords": [
    "regex",
    "benchmark",
    "nodejs21",
    "performance",
    "v8-engine",
    "non-backtracking",
    "linear-regex",
    "redos-prevention"
  ],
  "author": "Benchmark Project",
  "license": "MIT"
} 